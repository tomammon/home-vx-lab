{% include 'salttemplates/common/managed_by_salt.j2' %}

auto lo
iface lo inet loopback

auto eth0
iface eth0 inet dhcp
    vrf mgmt

auto mgmt
iface mgmt
    address 127.0.0.1/8
    address ::1/128
    vrf-table auto

auto swp1
iface swp1
    address 192.168.16.6/24

auto swp2
iface swp2
    address 192.168.26.6/24

auto swp3
iface swp3
    address 172.17.1.6/24

auto swp4
iface swp4
    address 172.17.2.6/24

auto swp5
iface swp5
    address 172.17.3.6/24

auto swp6
iface swp6
    address 10.7.103.6/24

auto swp7
iface swp7
    address 10.8.103.6/24
