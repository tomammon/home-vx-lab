{% include 'salttemplates/common/managed_by_salt.j2' %}
{% include 'salttemplates/common/interface_notes.j2' %}

auto lo
iface lo inet loopback
    address 10.100.1.5/32

auto eth0
iface eth0 inet dhcp
    vrf mgmt

auto mgmt
iface mgmt
    address 127.0.0.1/8
    address ::1/128
    vrf-table auto

auto swp1
iface swp1
    address 192.168.15.5/24

auto swp2
iface swp2
    address 192.168.25.5/24

auto swp3
iface swp3
    address 10.4.103.5/24

auto swp4
iface swp4
    address 10.5.103.5/24

auto swp5
iface swp5
    address 10.6.103.5/24
