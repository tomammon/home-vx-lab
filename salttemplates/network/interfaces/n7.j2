auto swp4
iface swp4
    address 172.17.2.7/24

auto swp5
iface swp5
    address 172.17.3.7/24

auto swp6
iface swp6
    address 10.9.103.7/24

auto swp7
iface swp7
    address 10.10.103.7/24

